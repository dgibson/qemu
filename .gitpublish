# The default profile, for qemu-kvm-rhev BZs
[gitpublishprofile "default"]
base = rhv7/master-2.12.0
prefix = RHEL-7.8 qemu-kvm-rhev PATCH
to = rhvirt-patches@redhat.com
suppresscc = all
message = True

