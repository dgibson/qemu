# The default profile
[gitpublishprofile "default"]
base = vp/master-4.0.0
prefix = RHEL-AV qemu-kvm PATCH
to = rhvirt-patches@redhat.com
suppresscc = all
message = True
blurb-template = BZ: <XYZ>
blurb-template = BRANCH: vp/master-4.0.0
blurb-template = UPSTREAM: <upstream status>
blurb-template = BREW: <brew id>
blurb-template = *** YOUR MESSAGE HERE ***
